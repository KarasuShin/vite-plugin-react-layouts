{
  "name": "vite-plugin-react-layouts",
  "version": "0.0.1",
  "author": "Karasu Shin",
  "description": "Router based layout plugin for Vite and React",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "scripts": {
    "build": "tsup src/index.ts --dts --format cjs,esm",
    "example:dev": "npm -C examples/spa run dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KarasuShin/vite-plugin-react-layouts.git"
  },
  "files": [
    "dist",
    "client.d.ts"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": "./*"
  },
  "keywords": [],

  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KarasuShin/vite-plugin-react-layouts/issues"
  },
  "homepage": "https://github.com/KarasuShin/vite-plugin-react-layouts#readme",
  "dependencies": {
    "fast-glob": "^3.2.11"
  },
  "peerDependencies": {
    "vite": "^2.9.10"
  },
  "devDependencies": {
    "@types/node": "^17.0.40",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^1.3.0",
    "eslint": "^8.17.0",
    "eslint-config-karasu": "^0.1.9",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "tsup": "^6.1.0",
    "typescript": "^4.7.3",
    "vite": "^2.9.10"
  }
}
